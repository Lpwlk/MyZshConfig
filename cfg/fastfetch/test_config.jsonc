            "type": "title",
            "key": "{$2}{#32}{icon} User",
            "format": "{$1}{user-name}  {#2}[{home-dir}]"
        },
        {
            "type": "users",
            "key": "{$2}{#32}{icon} Users",
            "myselfOnly": false,
            "format": "{$1}{1}@{host-name}{/host-name}localhost{/}{?client-ip}  {#2}[IP:{client-ip}]{?}  [Login time: {login-time}]"
        },
        {
            "type": "datetime",
            "key": "{$2}{#32}{icon} Datetime",
            "format": "{$1}{year}-{month-pretty}-{day-in-month} {hour-pretty}:{minute-pretty}:{second-pretty}  {#2}[{weekday}] [W{week}] [UTC{offset-from-utc}]"
        },
        {
            "type": "title",
            "key": "{$2}{#33}{icon} Host",
            "format": "{$1}{host-name}"
        },
        {
            "type": "host",
            "key": "{$2}{#33}{icon} Machine",
            "format": "{$1}{name}  {#2}{version}"
        },
        {
            "type": "os",
            "key": "{$2}{#33}{icon} OS",
            "format": "{$1}{?pretty-name}{pretty-name}{?}{/pretty-name}{name}{/} {codename}  {#2}[v{version}] [{arch}]"
        },
        {
            "type": "kernel",
            "key": "{$2}{#33}{icon} Kernel",
            "format": "{$1}{sysname}  {#2}[v{release}]"
        },
        {
            "type": "uptime",
            "key": "{$2}{#33}{icon} Uptime",
            "format": "{$1}{?days}{days} Days + {?}{hours}:{minutes}:{seconds}"
        },
        {
            "type": "cpu",
            "key": "{$2}{#31}{icon} CPU",
            "showPeCoreCount": true,
            "temp": true,
            "format": "{$1}{name}  {#2}[C:{core-types}] [{freq-max}]"
        },
        {
            "type": "gpu",
            "key": "{$2}{#31}{icon} GPU",
            "detectionMethod": "auto",
            "driverSpecific": true,
            "format": "{$1}{name}  {#2}[C:{core-count}]{?frequency} [{frequency}]{?} {#2}[{type}]"
        },
        {
            "type": "memory",
            "key": "{$2}{#31}{icon} Memory",
            "format": "{$1}{used} / {total} ({percentage})"
        },
        {
            "type": "disk",
            "key": "{$2}{#31}{icon} Disk",
            "format": "{$1}{size-used} / {size-total} ({size-percentage})"
        },
        {
            "type": "poweradapter",
            "key": "{$2}{#31}{icon} Power",
            "format": "{$1}{name}"
        },
        {
            "type": "terminal",
            "key": "{$2}{#35}{icon} Terminal",
            "format": "{$1}{pretty-name}  {#2}[{version}] [PID:{pid}]"
        },
        {
            "type": "terminalfont",
            "key": "{$2}{#35}{icon} Font",
            "format": "{$1}{name}  {#2}[{size}]"
        },
        {
            "type": "shell",
            "key": "{$2}{#35}{icon} Shell",
            "format": "{$1}{pretty-name}  {#2}[v{version}] [PID:{pid}]"
        },
        {
            // localip IPv4
            "type": "localip",
            "key": "{$2}{#34}{icon} Local IPv4",
            "showPrefixLen": true,
            "showIpv4": true,
            "showIpv6": false,
            "showMtu": true,
            "format": "{$1}{ifname}: {ipv4}  {#2}[MTU:{mtu}]"
        },
        {
            // localip IPv6
            "type": "localip",
            "key": "{$2}{#34}{icon} Local IPv6",
            "showPrefixLen": true,
            "showIpv4": false,
            "showIpv6": true,
            "showMtu": true,
            "format": "{$1}{ifname}: {ipv6}  {#2}[MTU:{mtu}]"
        },
        {
            "type": "publicip",
            "key": "{$2}{#34}{icon} Public IPv4",
            "ipv6": false,
            "format": "{$1}{ip}  {#2}[{location}]"
        },
        {
            "type": "publicip",
